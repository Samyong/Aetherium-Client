<Window x:Class="Aetherium_Launcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Aetherium Launcher" Height="768" Width="1366" WindowStyle="None" Background="DarkGray">
    <Grid>
        <Grid HorizontalAlignment="Left" Height="467" VerticalAlignment="Top" Width="1030" Margin="328,0,0,0">
            <WebBrowser HorizontalAlignment="Left" Height="633" VerticalAlignment="Top" Width="1030" Margin="0,28,0,-194" Source="http://aetherium.us"/>
            <Ellipse x:Name="ellipses"  Fill="Gray"  HorizontalAlignment="Left" Height="28" Margin="996,0,0,0" Stroke="Black" VerticalAlignment="Top" Width="28" MouseLeftButtonDown="Ellipse_MouseLeftButtonDown" MouseEnter="Ellipse_MouseEnter" MouseLeave="Ellipse_MouseLeave" />


            <Label MouseLeftButtonDown="Ellipse_MouseLeftButtonDown" Content="X" HorizontalAlignment="Left" Margin="1002,1,0,0" VerticalAlignment="Top" Width="23"/>
        </Grid>

    </Grid>
</Window>
